{
    "private": true,
    "name": "@arkecosystem/core-consensus",
    "description": "agnostic core-consensus plugin",
    "version": "0.0.1",
    "contributors": [
        "Sarang, Vasishth <sarang.vasishth@gmail.com>"
    ],
    "license": "MIT",
    "main": "dist/index",
    "types": "dist/index",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "yarn clean && yarn compile",
        "build:watch": "yarn clean && yarn compile -w",
        "build:docs": "../../node_modules/typedoc/bin/typedoc --out docs src",
        "clean": "rimraf dist",
        "compile": "node ../../node_modules/typescript/bin/tsc",
        "prepublishOnly": "yarn build"
    },
    "dependencies": {
        "@arkecosystem/core-kernel": "3.1.1",
        "@arkecosystem/crypto": "3.1.1"
    },
    "engines": {
        "node": ">=10.x"
    },
    "publishConfig": {
        "access": "public"
    }
}
